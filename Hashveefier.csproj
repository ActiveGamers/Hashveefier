<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.0.0</Version>
    <Authors>ActiveGamers</Authors>
    <Description>Simple SHA-256 file hasher and verifier</Description>
    <ApplicationIcon>Resources\Images\Icons\icons8-hash-96.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Resources\Images\Icons\icons8-hash-96.ico" />
  </ItemGroup>

</Project>